<%= render 'header' %>

<table class="table">
<% @commits.each do |commit| %>
<tr><td>
<div><%= commit[:subject] %></div>
<div><%= commit[:author] %></div>
</td><td>
<%= commit[:hash][0..10] %><br>
<a href="/<%= current_user.name %>/<%= @repository.name %>/tree/<%= commit[:hash] %>">Browse Code</a>
</td>
</tr>
<% end %>
</table>
