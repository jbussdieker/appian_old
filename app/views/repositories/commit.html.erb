<%= render 'mastercrumbs' %>
<br>
<%= render 'header' %>

<table class="table">
<tr><td>
<div><%= @commit.message %></div>
<div><%= @commit.author.name %></div>
</td><td>
<%= @commit.sha[0..10] %><br>
<a href="/<%= current_user.name %>/<%= @repository.name %>/tree/<%= @commit.sha %>">Browse Code</a>
</td>
</tr>
</table>
<pre>
<%= @commit.parent.diff(@commit) %>
</pre>
